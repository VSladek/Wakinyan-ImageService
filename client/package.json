  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:consumer:android": "eas build --platform android --profile production-consumer-android",
    "build:consumer:ios": "eas build --platform ios --profile production-consumer-ios",
    "build:admin:android": "eas build --platform android --profile production-admin-android",
    "build:admin:ios": "eas build --platform ios --profile production-admin-ios",
    "build:consumer": "npm run build:consumer:android && npm run build:consumer:ios",
    "build:admin": "npm run build:admin:android && npm run build:admin:ios",
    "build": "npm run build:consumer && npm run build:admin"
  },
